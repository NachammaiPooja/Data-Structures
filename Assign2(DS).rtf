void del(struct detail p[],int n,char nam[])\par
\{      int j,flag=0;\par
    for(int i=0;i<n;i++)\{\par
        if(strcmp(p[i].name,nam)==0)\par
        \{flag=0;\par
            j=i;\par
            for(int i=j;i<n;i++)\{\par
                p[i]=p[i+1];\par
