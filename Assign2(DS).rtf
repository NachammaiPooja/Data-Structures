void insmid(struct detail p[],int n,int r)\par
\{     int j,flag=0;\par
    for(int i=0;i<n;i++)\{\par
        if(p[i].regno==r)\par
        \{flag=0;\par
            j=i;\par
            for(int i=n;i>j+1;i--)\{\par
                p[i]=p[i-1];\}\par
            printf("\\nEnter the new details ");\par
            printf("\\n Register number ");\par
            scanf("%d",&p[j+1].regno);\par
            printf("\\n Name  ");\par
            scanf(" %s",p[j+1].name);\par
            printf("\\n Mark 1 ");\par
            scanf("%d",&p[j+1].m1);\par
            printf("\\n Mark 2 ");\par
            scanf("%d",&p[j+1].m2);\par
            printf("\\n Mark 3 ");\par
            scanf("%d",&p[j+1].m3);\par
            printf("\\n Mark 4 ");\par
            scanf("%d",&p[j+1].m4);\par
            printf("\\n Mark 5 ");\par
            scanf("%d",&p[j+1].m5);\par
            n++;\par
            display(p,n);\par
            break;\par
        \}\par
        else\par
            flag=1;\par
    \}\par
    if(flag!=0)\{\par
        printf("Register number not found");\par
    \}\par
\}\par
